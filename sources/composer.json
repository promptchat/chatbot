{
    "name": "promptchat/chatbot",
    "description": "White label chatbot and live chat",
    "keywords": ["chatbot", "live chat", "promptchat"],
    "license": "OEM",
    "version": "1.0.2",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "ext-json": "*",
        "ext-zip": "*",
        "adellantado/botman-viber-driver": "dev-master",
        "aws/aws-sdk-php": "^3.67",
        "bavix/laravel-wallet": "^4.0",
        "botman/botman": "^2.5",
        "botman/driver-facebook": "^1.10",
        "botman/driver-telegram": "^1.5",
        "botman/driver-twilio": "^1.2",
        "botman/driver-web": "^1.5",
        "botman/studio-addons": "^1.5",
        "doctrine/dbal": "^2.7",
        "fideloper/proxy": "^4.0",
        "geoip2/geoip2": "^2.9",
        "kreait/laravel-firebase": "^1.3",
        "kyslik/column-sortable": "5.8.*",
        "laravel/framework": "5.8.*",
        "laravel/tinker": "^1.0",
        "matthiasmullie/minify": "^1.3",
        "maxmind-db/reader": "^1.4",
        "paypal/rest-api-sdk-php": "*",
        "predis/predis": "^1.1",
        "simplesoftwareio/simple-qrcode": "^2.0",
        "staudenmeir/eloquent-json-relations": "^1.1",
        "twilio/sdk": "5.15",
        "waavi/translation": "2.3.x"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.6",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "exclude-from-classmap": ["vendor/botman/botman/src/Messages/Incoming/IncomingMessage.php"],
        "files": ["app/Overrides/IncomingMessage.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
