{
    "name": "promptchat/chatbot",
    "description": "White label chatbot and live chat",
    "keywords": ["chatbot", "live chat", "promptchat"],
    "license": "OEM",
    "version": "0.3.6",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "aws/aws-sdk-php": "^3.67",
        "doctrine/dbal": "^2.7",
        "fideloper/proxy": "^4.0",
        "geoip2/geoip2": "^2.9",
        "laravel/framework": "5.8.*",
        "kyslik/column-sortable": "5.8.*",
        "laravel/tinker": "^1.0",
        "matthiasmullie/minify": "^1.3",
        "maxmind-db/reader": "^1.4",
        "paypal/rest-api-sdk-php": "^1.14",
        "predis/predis": "^1.1",
        "waavi/translation": "2.3.x"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.6",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
