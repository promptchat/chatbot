{
    "name": "promptchat/installer.base",
    "type": "project",
    "version": "2.17.14",
    "description": "WhiteLabel chatbot system",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-zip": "*",
        "aws/aws-sdk-php": "^3.158",
        "botman/botman": "^2.6",
        "botman/driver-facebook": "^1.11",
        "botman/driver-telegram": "^1.6",
        "botman/driver-twilio": "^1.2",
        "botman/driver-web": "^1.5",
        "botman/studio-addons": "^1.6",
        "fideloper/proxy": "^4.2",
        "friendsofphp/php-cs-fixer": "^2.16",
        "fruitcake/laravel-cors": "^2.0",
        "geoip2/geoip2": "^2.11",
        "google/apiclient": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "intervention/image": "^2.5",
        "laravel/cashier": "^13.16",
        "laravel/fortify": "^1.6",
        "laravel/framework": "^8.0",
        "laravel/tinker": "^2.0",
        "league/html-to-markdown": "^4.10",
        "matthiasmullie/minify": "^1.3",
        "predis/predis": "^1.1",
        "propaganistas/laravel-phone": "^4.3",
        "psr/cache": "^1.0",
        "staudenmeir/eloquent-json-relations": "^1.5",
        "theardent/botman-viber-driver": "dev-master",
        "twilio/sdk": "^5.15"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.8",
        "facade/ignition": "^2.3.6",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "nunomaduro/larastan": "^0.6.10",
        "phpstan/phpstan": "^0.12.58",
        "phpunit/phpunit": "^9.3",
        "squizlabs/php_codesniffer": "^3.5",
        "tightenco/tlint": "^5.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "thecodingmachine/discovery": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "exclude-from-classmap": ["vendor/botman/botman/src/Messages/Incoming/IncomingMessage.php"],
        "files": ["app/Overrides/IncomingMessage.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "php-cs": [
            "php-cs-fixer fix --config=.php_cs.php --diff --format=txt"
        ],
        "pstan": [
            "vendor/bin/phpstan analyse app -c phpstan.neon"
        ]
    }
}
